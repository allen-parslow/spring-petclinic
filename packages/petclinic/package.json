{
  "name": "petclinic",
  "author": "React Implementation by Allen Parslow",
  "description": "spring-petclinic using webpack + React + aws",
  "private": true,
  "license": "APACHE 2",
  "version": "1.0.0",
  "keywords": [
    "webpack",
    "docker",
    "react"
  ],
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.js --port 3000",
    "build": " webpack --config webpack.prod.js",
    "dev-build": "NODE_ENV=development webpack --config webpack.dev.js",
    "clean": "[ -d 'dist/' ] && rm -r dist/ || :",
    "test": "jest"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.1.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-15": "^1.0.0",
    "eslint": "^4.7.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-enzyme": "^4.0.0",
    "json-loader": "^0.5.7",
    "lerna": "^2.4.0",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "sass-lint": "^1.11.1",
    "sass-loader": "^6.0.6",
    "sasslint-webpack-plugin": "^1.0.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "i18n-lite": "^1.0.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/enzyme/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/scripts/mockStyle.js"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/test-setup.js"
  }
}
